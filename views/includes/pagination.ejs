<section class="pagination">
    <a href="?page=1" class="<%= currentPage ===1 ? 'disabled' : '' %>">&lt;&lt;</a>
    <a href="?page=<%= previousPage %>" class="<%= currentPage ===1 ? 'disabled' : '' %>">&lt;</a>

    <% if (hasPreviousPage && (previousPage - 1> 1)) { %>
        <a href="?page=<%= previousPage - 1 %>">
            <%= previousPage - 1 %>
        </a>
        <% } %>
            <% if (hasPreviousPage) { %>
                <a href="?page=<%= previousPage %>">
                    <%= previousPage %>
                </a>
                <% } %>
                    <a href="?page=<%= currentPage %>" class="active">
                        <%= currentPage %>
                    </a>
                    <% if (hasNextPage) { %>
                        <a href="?page=<%= nextPage %>">
                            <%= nextPage %>
                        </a>
                        <% } %>
                            <% if (hasNextPage && (lastPage - currentPage> 1)) { %>
                                <a href="?page=<%= nextPage + 1 %>">
                                    <%= nextPage + 1 %>
                                </a>
                                <% } %>

                                    <a href="?page=<%= nextPage %>"
                                        class="<%= currentPage === lastPage ? 'disabled' : '' %>">&gt;</a>
                                    <a href="?page=<%= lastPage %>"
                                        class="<%= currentPage === lastPage ? 'disabled' : '' %>">&gt;&gt;</a>
</section>